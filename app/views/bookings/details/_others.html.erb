    <tr>
      <td>		    
		    <div class="control-group">
		      <%= f.label :notes, :class => 'control-label' %>
		      <div class="controls">
		        <%= f.text_area :notes, :class => 'text_field', :style => "width:256px; height:119px;" %>
		      </div>
