<table>
    <tr>
      <td>	
		    <div class="control-group">
		      <%= f.label :death_certificate, :class => 'control-label' %>
		      <div class="controls">
		        <%= f.check_box :death_certificate, :class => 'check_box' %>
		      </div>
		    </div>	
      </td>
      <td>		
		    <div class="control-group">
		      <%= f.label :own_clergy, :class => 'control-label' %>
		      <div class="controls">
		        <%= f.check_box :own_clergy, :class => 'check_box' %>
		      </div>
		    </div>	
      </td> 
    </tr> 
    <tr>
      <td>		    
		    <div class="control-group">
		      <%= f.label :disease_id, :class => 'control-label' %>
		      <div class="controls">
		        <%= f.select :disease_id, options_for_select(get_model_data("Disease"), (@o_single.disease.id unless @o_single.disease.nil?))  %>
		      </div>
		    </div>     
      </td>
      <td>		    
		    <div class="control-group">
		      <%= f.label :clergy_name, :class => 'control-label' %>
		      <div class="controls">
		        <%= f.text_field :clergy_name, :class => 'text_field' %>
		      </div>
		    </div>	
      </td> 
    </tr> 
    <tr>
      <td>		    
		    <div class="control-group">
		      <%= f.label :chapel_id, :class => 'control-label' %>
		      <div class="controls">
		        <%= select_tag "chapel_ids", options_for_select(get_chapel_data, (@o_single.chapel_ids.split(",") unless @o_single.chapel_ids.blank?)), :include_blank => true, :multiple => true  %>
		      </div>
		    </div>  
		
		    <div class="control-group">
		      <%= f.label :chapel_time_from, :class => 'control-label' %>
		      <div class="controls">
			 			<div id="chapel_time_from" class="input-append">
			 				<%= f.text_field :chapel_time_from, :value => (@o_single.chapel_time_from.to_s(:default_date_time) if @o_single.chapel_time_from), :class => 'text_field' %>
					    <span class="add-on">
					      <i data-time-icon="icon-time" data-date-icon="icon-calendar">
					      </i>
					    </span> 				
			 			</div>	        
		      </div>
		    </div>
      </td> 
    </tr> 
    <tr>
      <td>		    	
		    <div class="control-group">
		      <%= f.label :chapel_time_to, :class => 'control-label' %>
		      <div class="controls">
			 			<div id="chapel_time_to" class="input-append">
			 				<%= f.text_field :chapel_time_to, :value => (@o_single.chapel_time_to.to_s(:default_date_time) if @o_single.chapel_time_to), :class => 'text_field' %>
					    <span class="add-on">
					      <i data-time-icon="icon-time" data-date-icon="icon-calendar">
					      </i>
					    </span> 				
			 			</div>	        
		      </div>
		    </div>	
      </td>
      <td>		    
		    <div class="control-group">
		      <%= f.label :room_id, :class => 'control-label' %>
		      <div class="controls">
		        <%= select_tag "room_ids", options_for_select(get_room_data, (@o_single.room_ids.split(",") unless @o_single.room_ids.blank?)), :include_blank => true, :multiple => true  %>
		      </div>
		    </div>        
      </td> 
    </tr> 
    <tr>
      <td>		    
		    <div class="control-group">
		      <%= f.label :room_time_from, :class => 'control-label' %>
		      <div class="controls">
			 			<div id="room_time_from" class="input-append">
			 				<%= f.text_field :room_time_from, :value => (@o_single.room_time_from.to_s(:default_date_time) if @o_single.room_time_from), :class => 'text_field' %>
					    <span class="add-on">
					      <i data-time-icon="icon-time" data-date-icon="icon-calendar">
					      </i>
					    </span> 				
			 			</div>	        
		      </div>
		    </div>
      </td>
      <td>		    	
		    <div class="control-group">
		      <%= f.label :room_time_to, :class => 'control-label' %>
		      <div class="controls">
			 			<div id="room_time_to" class="input-append">
			 				<%= f.text_field :room_time_to, :value => (@o_single.room_time_to.to_s(:default_date_time) if @o_single.room_time_to), :class => 'text_field' %>
					    <span class="add-on">
					      <i data-time-icon="icon-time" data-date-icon="icon-calendar">
					      </i>
					    </span> 				
			 			</div>	        
		      </div>
		    </div>	  
      </td> 
    </tr> 
    <tr>
      <td>		    
